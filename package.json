{
  "name": "rxjs-stream-monitor",
  "version": "2.0.0",
  "description": "stream monitor utility for rxjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehog90/rxjs-stream-monitor.git"
  },
  "keywords": [
    "rxjs",
    "stream",
    "monitor"
  ],
  "author": "Bence Ujj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ehog90/rxjs-stream-monitor/issues"
  },
  "homepage": "https://github.com/ehog90/rxjs-stream-monitor#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "rxjs": "^7.2.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "rxjs": "^5 || ^6 || ^7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
