{
  "name": "rxjs-stream-monitor",
  "version": "3.0.0",
  "description": "stream monitor utility for rxjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
